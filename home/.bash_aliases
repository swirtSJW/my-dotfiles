
# misc
alias grep='grep --color=auto'
alias dr='drush'

# dir listing
alias l='ls -alh'
alias la='ls -A'
alias ll='ls -alF'
alias ls='ls --color=auto'
alias lt='l -rt'
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

# git
alias av='git branch -av'
alias bv='git branch -v'
alias rv='git remote -v'
alias staged='git diff --staged'
alias backone='git co HEAD^ && git show --stat'

